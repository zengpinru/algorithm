{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      false
    ],
    "semicolon": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "space-before-function-paren": [
      false
    ],
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}